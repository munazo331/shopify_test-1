    #AFRegistrationFormPreload {
        background: url({{ 'lightbox/v204/loading.gif' | global_asset_url }}) center no-repeat;
        background-size: 30px;
        text-indent: -9999999px;
        display: block;
        width: 100%;
        height: 200px;
    }
